<% if @course.has_trainee? %>
	<% if @course.started? %>
		<%= link_to "Finish Course", "#", class: "btn btn-large btn-primary" %>
	<% else %>
		<%= link_to "Start Course", [:admin, current_supervisor, @course], method: :patch,
		  confirm: "Start this course?", class: "btn btn-large btn-primary" %>
	<% end %>
<% end %>